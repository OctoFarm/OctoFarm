{
  "name": "@notexpectedyet/octofarm-client",
  "version": "1.5.0",
  "description": "The website client for OctoFarm bundled using webpack.",
  "scripts": {
    "build": "webpack --mode=production",
    "webpack": "webpack",
    "dev": "webpack --watch",
    "release": "release-it --ci --config release-it.js",
    "release:dry": "release-it --dry-run --ci --config release-it.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OctoFarm/OctoFarm"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "octofarm": "./index.js"
  },
  "keywords": [
    "octofarm",
    "frontend",
    "ui",
    "website",
    "javascript",
    "3d-printing"
  ],
  "author": "James Mackay (NotExpectedYet)",
  "license": "ISC",
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@popperjs/core": "^2.11.5",
    "apexcharts": "^3.35.0",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "before-build-webpack": "^0.2.11",
    "bootbox": "^5.5.2",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "file-loader": "^6.2.0",
    "flatted": "^3.1.1",
    "gridstack": "^5.1.0",
    "html-loader": "^3.1.0",
    "jquery": "^3.5.1",
    "litepicker": "^2.0.12",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.6.0",
    "noty": "^3.2.0-beta",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "release-it": "^15.0.0",
    "semantic-release": "^19.0.2",
    "source-map-loader": "^2.0.1",
    "webpack": "5.73.0",
    "webpack-cli": "^4.7.2"
  },
  "engine-strict": true,
  "engines": {
    "npm": ">= 6.10.0"
  }
