{
  "name": "octofarm-client",
  "version": "1.8.0-beta.2",
  "description": "The website client for OctoFarm bundled using webpack.",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OctoFarm/OctoFarm"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "octofarm": "./index.js"
  },
  "keywords": [
    "octofarm",
    "frontend",
    "ui",
    "website",
    "javascript",
    "3d-printing"
  ],
  "author": "James Mackay (NotExpectedYet)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "apexcharts": "^3.35.5",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "before-build-webpack": "^0.2.12",
    "bootbox": "^5.5.2",
    "bootstrap": "^4.3.1",
    "browserlist": "^1.0.1",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.0",
    "flatted": "^3.2.7",
    "gridstack": "^6.0.2",
    "jquery": "^3.6.1",
    "litepicker": "^2.0.12",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.1",
    "noty": "^3.2.0-beta",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "source-map-loader": "^4.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "engine-strict": true,
  "engines": {
    "npm": ">= 6.10.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "core-js": "^3.25.3"
  }
}
