{
  "name": "octofarm-client",
  "version": "1.6.6",
  "description": "The website client for OctoFarm bundled using webpack.",
  "scripts": {
    "build": "webpack --mode=production",
    "webpack": "webpack",
    "dev": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OctoFarm/OctoFarm"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "octofarm": "./index.js"
  },
  "keywords": [
    "octofarm",
    "frontend",
    "ui",
    "website",
    "javascript",
    "3d-printing"
  ],
  "author": "James Mackay (NotExpectedYet)",
  "license": "ISC",
  "devDependencies": {
    "apexcharts": "^3.35.0",
    "popper.js": "^1.16.1",
    "axios": "^0.21.1",
    "bootbox": "^5.5.2",
    "bootstrap": "^4.3.1",
    "flatted": "^3.1.1",
    "gridstack": "^4.2.2",
    "jquery": "^3.5.1",
    "litepicker": "^2.0.12",
    "lodash": "^4.17.21",
    "noty": "^3.2.0-beta",
    "path": "^0.12.7"
  },
  "engine-strict": true,
  "engines": {
    "npm": ">= 6.10.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
