<% const hidden_not_development_mode = (development_mode) ? "" : "d-none" %>

    <div class="btn-group float-right <%=hidden_not_development_mode%>">
        <button type="button" class="btn btn-outline-danger dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            <i class="fab fa-dev"></i> Development Actions
        </button>
        <div class="dropdown-menu">
            <button
                    title="Enable Virtual printer on all online instances..."
                    id="blkEnableVirtualPrinter"
                    type="button"
                    class="dropdown-item"
                    href="#"
                    aria-expanded="false"
            >
                <i class="fas fa-vr-cardboard"></i> Enable Virtual Printer...
            </button>
        </div>
    </div>
