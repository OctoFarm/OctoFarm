                            <th scope="col">Script</th>
                            <th scope="col">Message</th>
                        </tr>
                        </thead>
                        <tbody id="printerAltersTableBody"
                               style="height:100%; overflow-y:auto;">

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="tab-pane fade" id="printer-power" role="tabpanel"
