</div>
<div class="row">
  <div id="wrapper" class="col-lg-12 table-responsive-sm table-responsive-md d-none">
    <table id="historyTableTop" class="table table-dark text-center">
      <thead>
        <tr>
          <th class="table-dark sticky-table table-dark" scope="col">#</th>
          <th class="table-dark sticky-table table-dark" scope="col">State</th>
          <th class="table-dark sticky-table table-dark" scope="col">Printer</th>
          <th class="table-dark sticky-table table-dark" scope="col" width="400px">File Name</th>
          <th class="table-dark sticky-table table-dark" scope="col">Start Date</th>
          <th class="table-dark sticky-table table-dark" scope="col">End Date</th>
          <th class="table-dark sticky-table table-dark" scope="col">Print Time</th>
          <th class="table-dark sticky-table table-dark" scope="col">Printer Cost</th>
          <th class="table-dark sticky-table table-dark" scope="col" width="400px">Spool</th>
          <th class="table-dark sticky-table table-dark" scope="col" width="400px">Filament Usage</th>
          <th class="table-dark sticky-table table-dark" scope="col" width="400px">Filament Cost</th>
          <th class="table-dark sticky-table table-dark" scope="col">Total Cost</th>
          <th class="table-dark sticky-table table-dark" scope="col">Cost Per Hour</th>
          <th class="table-dark sticky-table table-dark" scope="col" width="400px">
            Notes
          </th>
          <th class="sticky-table table-dark" scope="col"></th>
        </tr>
      </thead>
      <tbody id="historyTable" data-jplist-group="history">
        <% for(let i = 0; i < history.length; i++){ %>
        <tr data-jplist-item>
          <td class="index">
                  <%= history[i].index %>
          </td>
          <td>
            <%- history[i].state %>
          </td>
          <td class="printer <%= history[i].printer.replace(/[^a-zA-Z0-9]/g, "_") %>"><%=
            history[i].printer %>
          </td>
          <% let path = history[i].file.path.substring(0, history[i].file.path.lastIndexOf("/"))%>
          <td title="<%= path %>" class="file <%= history[i].file.name.replace(/[^a-zA-Z0-9]/g, "_") %> <%= path.replace(/\//g, "_") %>" class="text-wrap">
            <%= history[i].file.name.replace(/_/g," ") %>
          </td>
          <td class="start">
            <%= history[i].startDate %>
          </td>
          <td class="end">
            <%= history[i].endDate %>
          </td>
          <td>
            <span class="d-none time"><%= history[i].printTime %></span>
            <span><%= helpers.generateTime(history[i].printTime) %></span>
            <br><code><%= history[i].printHours %></code>
          </td>
          <td class="printerCost" id="printerCost-<%= history[i]._id %>">
            <%= history[i].printerCost %>
          </td>
          <% let spoolTypes = "" %>
          <% if(history[i].spools !== null){ %>
            <% history[i].spools.forEach((spool,index) => { %>
            <% spoolTypes += spool[Object.keys(spool)[0]].type+" " %>
          <% })%>
          <% } %>
          <td class="spool <%= spoolTypes %>" id="spool-<%= history[i]._id %>">
            <% if(history[i].spools !== null){ %>
              <% history[i].spools.forEach((spool,index) => { %>
                <%if(spool[Object.keys(spool)[0]].spoolName !== null){%>
                <b><%= spool[Object.keys(spool)[0]].toolName %></b>: <p><%= spool[Object.keys(spool)[0]].spoolName %></p>
                <%}else{%>
                <b><%= spool[Object.keys(spool)[0]].toolName %></b>: <p>(No Spool)</p>
              <%}%>
              <% })%>
            <% } %>
          </td>
          <td >
            <span class="usage" id="usage-<%= history[i]._id %>">
              <% if(history[i].spools !== null){ %>
                <% history[i].spools.forEach((spool,index) => { %>
                <%if(spool[Object.keys(spool)[0]].spoolName !== null){%>
                  <b><%= spool[Object.keys(spool)[0]].toolName %></b>: <p><%= spool[Object.keys(spool)[0]].weight %>g / <%= spool[Object.keys(spool)[0]].length %>m</p>
                <%}else{%>
                  <b><%= spool[Object.keys(spool)[0]].toolName %></b>: <p>(No Spool)</p>
                <%}%>
                <% })%>
              <% } %>
            </span>
            <span class="d-none totalUsageMeter" id="totalUsageMeter-<%= history[i]._id %>">
              <%= history[i].totalLength %>
            </span>
            <span class="d-none totalUsageGrams" id="totalUsageGrams-<%= history[i]._id %>">
              <%= history[i].totalWeight %>
            </span>
          </td>
          <td>
            <span class="cost" id="cost-<%= history[i]._id %>">
              <% if(history[i].spools !== null){ %>
                <% history[i].spools.forEach((spool,index) => { %>
                <%if(spool[Object.keys(spool)[0]].spoolName !== null){%>
                  <b><%= spool[Object.keys(spool)[0]].toolName %></b>: <p><%= spool[Object.keys(spool)[0]].cost %></p>
                <%}else{%>
                  <b><%= spool[Object.keys(spool)[0]].toolName %></b>: <p>(No Spool)</p>
                <%}%>
                <% })%>
              <% } %>
            </span>
            <span class="d-none filamentCost" id="totalFilamentCost-<%= history[i]._id %>">
              <%= history[i].spoolCost %>
            </span>
          </td>
          <td class="totalCost" id="totalCost-<%= history[i]._id %>">
            <%= history[i].totalCost %>
          </td>
          <td class="costPerHour" id="costPerHour-<%= history[i]._id %>">
            <%= history[i].costPerHour %>
          </td>
          <td class="notes" class="text-wrap" id="note-<%= history[i]._id %>">
            <%= history[i].notes %>
          </td>
          <td>
            <div class="btn-group" role="group" aria-label="Basic example">
              <button
                type="button"
                class="btn btn-info btn-small historyEdit bg-colour-1"
                id="<%= history[i]._id %>"
                data-toggle="modal"
                data-target="#historyModal"
              >
                <i class="fas fa-eye"></i>
              </button>

              <button
                type="button"
                class="btn btn-danger btn-small historyDelete"
                id="<%= history[i]._id %>"
              >
                <i class="fas fa-trash"></i>
              </button>
            </div>
          </td>
        </tr>
        <% } %>
      </tbody>

    </table>

  </div>
</div>
  <script src="/assets/js/historyRunner.min.js" type="module"></script>
<% }else{ %>
  <div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body text-center">
        <h1>No History yet</h1>
        <p>
          Come back here when you've completed some prints.
        </p>
        <h1><img width="500px" src="images/logo.png" /></h1>
      </div>
    </div>
  </div>
<% } %>
