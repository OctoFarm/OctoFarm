<% if(typeof printerCount != 'undefined' && printerCount > 0 ) { %>
    <div class="col-12">
        <div class="card mb-4 mt-2">
            <div class="card-body p-2 bg-dark">
                <div class="row mt-3">
                    <div class="col-12 col-md-6 col-lg-5">
                        <div class="alert alert-danger p-1 m-0" style="font-size:1rem; opacity: 0.9;">
                            <small><b class="p-0">Finishing Today</b></small>
                            <small id="co-latest-finish-time-today"><b></b></small>
                        </div>
                        <div id="co-finish-today-list"
                             style="height:6rem; width:100%; overflow-y: scroll; font-family: monospace;">

                        </div>
                    </div>
                    <div class="col-12 col-md-6 col-lg-5">
                        <div class="alert alert-warning p-1 m-0" style="font-size:1rem; opacity: 0.9;">
                            <small><b class="p-0">Finishing Tomorrow</b></small>
                            <small id="co-latest-finish-time-tomorrow"><b></b></small>
                        </div>
                        <div id="co-finish-tomorrow-list"
                             style="height:6rem; width:100%; overflow-y: scroll; font-family: monospace;">

                        </div>
                    </div>
                    <div class="col-12 col-md-6 col-lg-2 text-center">
                        <div class="alert alert-warning" role="alert">
                            <strong id="co-active-print-count">
                                <div class="spinner-border spinner-border-sm" role="status">
                                    <span class="visually-hidden">Loading...</span>
                                </div>
                            </strong> Active<br>
                        </div>
                        <div class="alert alert-success" role="alert">
                            <strong id="co-complete-print-count">
                                <div class="spinner-border spinner-border-sm" role="status">
                                    <span class="visually-hidden">Loading...</span>
                                </div>
                            </strong> Complete<br>
                        </div>
                </div>
            </div>
        </div>


                <div>
                    <div>
                        <h2 id="headingOne">



                        </h2>
                        <div id="co-extra-info" class="accordion-collapse collapse" aria-labelledby="co-extra-info" data-bs-parent="#co-extra-info">

                        </div>
                        <div id="co-settings" class="accordion-collapse collapse" aria-labelledby="co-settings" data-bs-parent="#co-settings">
                            <div class="row mt-3">
                                <h4>Sorting</h4><hr>
                                <div class="col">
                                    <div class="form-check">
                                        <input id="ci-progress-up" class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios1" value="option1" checked>
                                        <label class="form-check-label" for="exampleRadios1">
                                            <i class="fas fa-sort-numeric-up"></i> Progress
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios2" value="option2">
                                        <label class="form-check-label" for="exampleRadios2">
                                            <i class="fas fa-sort-numeric-down"></i> Progress
                                        </label>
                                    </div>
                                </div>
                                <div class="col">
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios3" value="option3">
                                        <label class="form-check-label" for="exampleRadios3">
                                            <i class="fas fa-sort-numeric-up"></i> Time Remaining
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios4" value="option4">
                                        <label class="form-check-label" for="exampleRadios4">
                                            <i class="fas fa-sort-numeric-down"></i> Time Remaining
                                        </label>
                                    </div>
                                </div>
                                <div class="col">
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios5" value="option5">
                                        <label class="form-check-label" for="exampleRadios5">
                                            <i class="fas fa-sort-alpha-up"></i> File Name
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios6" value="option6">
                                        <label class="form-check-label" for="exampleRadios6">
                                            <i class="fas fa-sort-alpha-down"></i> File Name
                                        </label>
                                    </div>
                                </div>
                                <div class="col">
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios7" value="option7">
                                        <label class="form-check-label" for="exampleRadios7">
                                            <i class="fas fa-sort-numeric-up"></i> Sort Index
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios8" value="option8">
                                        <label class="form-check-label" for="exampleRadios8">
                                            <i class="fas fa-sort-numeric-down"></i> Sort Index
                                        </label>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>




            </div>
        </div>
    </div>
    <div id="currentOperationsBody" class="d-flex flex-row flex-wrap" style="min-height: 70px">

    </div>

<% }else{ %>
    <%- include('./addPrinters.ejs') %>
<% } %>
