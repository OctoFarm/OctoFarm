<div class="card text-center">
    <div class="card-body">
<div class="row">
    <div class="col-lg-12 text-center">
        <h4>Sponsor Links</h4><hr>
        <a type="button" class="btn btn-danger mb-2" href="https://www.patreon.com/NotExpectedYet" target="_blank"><i class="fab fa-patreon"></i> Patreon</a>
        <a type="button" class="btn btn-info mb-2" href="https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=QGBJML3E7268L&source=url" target="_blank"><i class="fab fa-paypal"></i> Paypal</a>
        <a type="button" class="btn btn-primary mb-2" href="https://buymeacoffee.com/OctoFarm" target="_blank"><i class="fas fa-mug-hot"></i> Buy Me a Coffee!</a>
    </div>
</div>
<div class="row">
    <hr><h4>Big thanks to the following Supporters!</h4><hr>
    <% patreonData.forEach(patreon => { %>
        <div class="col-sm-6 col-md-4 col-lg-3 text-center mb-2">
            <li class="list-group-item list-group-item-dark rounded px-1 py-2">
                <div class="patreon-user"> <img class="patreon-img" src="<%= patreon.thumb %>"></div>
                <small class="text-muted text-wrap"><%= patreon.name %></small>

            </li>
        </div>
    <% }) %>
</div>
    </div>
</div>