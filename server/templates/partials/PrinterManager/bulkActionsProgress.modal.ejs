<div class="col-12 sticky-top bg-dark">

    <div class="alert alert-info text-dark" role="alert" id="bulkActionInfoMessage">

    </div>
    <h4>Current Progress</h4>
    <div class="progress mb-2">
        <div id="bulkActionProgressBar" class="progress-bar bg-info" role="progressbar" style="width: 0%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">0%</div>
    </div>
    <h6> Complete: <span id="bulkActionCompleteCount"> 0 </span></h6>
    <h6> Warning: <span id="bulkActionWarning"> 0 </span></h6>
    <h6> Skipped: <span id="bulkActionSkipped"> 0 </span></h6>
    <h6> Error: <span id="bulkActionError"> 0 </span></h6>

</div>
<div class="col-12">
    <table class="table table-striped table-dark">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Printer Name</th>
            <th scope="col">Result</th>
            <th scope="col">Message</th>
        </tr>
        </thead>
        <tbody id="bulkActionResultsTable">

        </tbody>
    </table>
</div>


