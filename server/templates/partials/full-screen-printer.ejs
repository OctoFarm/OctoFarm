<div id="fullScreenDisplayElementWrapper" class="full-display-element d-none noBlue">
    <div id="drawCameraWrapper">

    </div>
    <div class="overlayFullCameraStatus">
        <ul class="list-group text-left mb-1" style="min-width: 200px; max-width: 200px;">
            <li class="list-group-item list-group-item-warning"><b>File Name</b></li>
            <li class="list-group-item text-truncate"><span id="fullScreenCameraFileName"> - </span></li>
        </ul>
        <ul class="list-group text-left mb-1" style="min-width: 200px; max-width: 200px;">
            <li class="list-group-item list-group-item-warning"><b>Progress </b></li>
            <li class="list-group-item"><b>Print Time: </b><span id="fullScreenCameraPrintTimeElapsed"> - </span></li>
            <li class="list-group-item"><b>Print Time Left: </b><span id="fullScreenCameraPrintTimeLeft"> - </span></li>
            <li class="list-group-item">
